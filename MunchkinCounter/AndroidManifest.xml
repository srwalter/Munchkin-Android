<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.slinkman.munchkin"
      android:versionName="1.2.1" android:versionCode="6">
    <application android:label="@string/app_name" android:debuggable="false" android:icon="@drawable/munchkinlogo">
        <activity android:name=".activity.Counter"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    
<activity android:name=".activity.Power"></activity>
<activity android:name=".activity.Dice" android:screenOrientation="portrait"></activity>
<activity android:name=".activity.Settings"></activity>
<activity android:name=".activity.Gear"></activity>
</application>
    <uses-sdk android:minSdkVersion="7" />

<uses-library android:name="libGoogleAnalytics.jar" />
<uses-library android:name="MunchkinPresenter-1.0-SNAPSHOT.jar" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.INTERNET" /> 
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest> 
